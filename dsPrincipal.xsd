                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        i.idingreso, t.apellidos + ' ' + t.nombre AS Trabajador, p.razon_social AS Proveedor, i.fecha, i.tipo_comprobante, i.serie, i.correlativo, i.estado, SUM(d.precio_compra * d.stock_inicial) AS Total, i.iva AS Impuesto
FROM            Detalle_ingreso AS d INNER JOIN
                         Ingreso AS i ON d.idingreso = i.idingreso INNER JOIN
                         proveedor AS p ON i.idproveedor = p.idproveedor INNER JOIN
                         trabajador AS t ON i.idtrabajador = t.idtrabajador
GROUP BY i.idingreso, t.apellidos + ' ' + t.nombre, p.razon_social, i.fecha, i.tipo_comprobante, i.serie, i.correlativo, i.estado, i.iva
HAVING        (i.fecha &gt;= @Param1 AND i.fecha &lt;= @Param2)
ORDER BY i.idingreso DESC</CommandText>
